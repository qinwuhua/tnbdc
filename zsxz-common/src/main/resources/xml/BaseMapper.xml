<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.hdsx.common.mapper.BaseMapper">

    <!-- 创建组织域下拉框 -->
    <select id = "createZzxxList" resultType="java.util.Map">
        SELECT * FROM DJSJ_ZZXX
    </select>

    <!-- 根据公司ID查人员信息 -->
    <select id = "createRyxxList" parameterType="String" resultType="java.util.Map">
        SELECT * FROM DJSJ_RYXX WHERE 1=1
        <if test="_parameter != null and _parameter !=''">
            AND RYXX_GSID  = #{_parameter}
        </if>
    </select>

</mapper>