            <if test="gcjsbyj != null" >gcjsbyj=#{gcjsbyj},</if>
            <if test="jyfzbyj != null" >jyfzbyj=#{jyfzbyj},</if>
            <if test="tbr != null" >tbr=#{tbr},</if>
