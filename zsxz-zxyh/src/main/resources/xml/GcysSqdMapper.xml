            <if test="htksgq != null" >htksgq,</if>
            <if test="htjsgq != null" >htjsgq,</if>
            <if test="yslx != null" >yslx,</if>
            <if test="sgfyj != null" >sgfyj,</if>
            <if test="jldwyj != null" >jldwyj,</if>
