                </if>
            </trim>;
        </foreach>
        COMMIT;
        END;
    </insert>

    <delete id="deleteYdjh" parameterType="string">
        DELETE FROM ZXYH_YDJH WHERE id IN
        <foreach collection="array" item="id" open="(" close=")" separator=",">
            #{id}
        </foreach>
    </delete>

    <delete id="deleteYdjhmx" parameterType="string">
        DELETE FROM ZXYH_YDJH_MX WHERE ydid IN
        <foreach collection="array" item="ydid" open="(" close=")" separator=",">
            #{ydid}
        </foreach>
    </delete>

    <update id="updateYdjh" parameterType="com.hdsx.zxyh.entity.Ydjh">
      update ZXYH_YDJH
      <set>
          <if test="djbh !=null and djbh !=''">
              djbh=#{djbh},
          </if>
          <if test="yd != null and yd !=''">
              yd=#{yd},
          </if>
          <if test="byjhwcqk !=null and byjhwcqk !=''">
              byjhwcqk=#{byjhwcqk},
          </if>
          <if test="xyjhwcqk != null and xyjhwcqk !=''">
              xyjhwcqk=#{xyjhwcqk},
          </if>
          <if test="bz !=null and bz !=''">
              bz=#{bz},
          </if>
          <if test="bzr !=null and bzr !=''">
              bzr=#{bzr},
          </if>
          <if test="bzrq !=null and bzrq !=''">
              bzrq=to_date(#{bzrq}, 'yyyy/mm/dd'),
          </if>
          <if test="bzdw !=null and bzdw !=''">
              bzdw=#{bzdw},
          </if>
          <if test="bzbm !=null and bzbm !=''">
              bzbm=#{bzbm},
          </if>
          <if test="sbzt != null and sbzt != ''">
              sbzt=#{sbzt},
          </if>
          <if test="pfzt != null and pfzt != ''">
              pfzt=#{pfzt},
          </if>
      </set>
      where id=#{id}
    </update>
