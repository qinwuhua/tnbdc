                <if test="item.sjksrq != null and item.sjksrq != ''">
                    sjksrq,
                </if>
                <if test="item.sjwcrq != null and item.sjwcrq != ''">
                    sjwcrq,
                </if>
            </trim>
            <trim prefix="values (" suffix=")" suffixOverrides=",">
                <if test="item.mxid !=null and item.mxid !=''">
                    #{item.mxid},
                </if>
                <if test="item.ydid !=null and item.ydid !=''">
                    #{item.ydid},
                </if>
                <if test="item.qdzh !=null and item.qdzh !=''">
                    #{item.qdzh},
                </if>
                <if test="item.zdzh != null and item.zdzh !=''">
                    #{item.zdzh},
                </if>
                <if test="item.jggc !=null and item.jggc !=''">
                    #{item.jggc},
                </if>
                <if test="item.fx !=null and item.fx !=''">
                    #{item.fx},
                </if>
                <if test="item.czfs !=null and item.czfs !=''">
                    #{item.czfs},
                </if>
                <if test="item.czcd !=null and item.czcd !=''">
                    #{item.czcd},
                </if>
                <if test="item.jhgcl !=null and item.jhgcl !=''">
                    #{item.jhgcl},
                </if>
                <if test="item.jhksrq != null and item.jhksrq != ''">
                    to_date(#{item.jhksrq}, 'yyyy/mm/dd'),
                </if>
                <if test="item.jhwcrq != null and item.jhwcrq != ''">
                    to_date(#{item.jhwcrq}, 'yyyy/mm/dd'),
                </if>
                <if test="item.sjgcl != null and item.sjgcl != ''">
                    #{item.sjgcl},
                </if>
                <if test="item.sjksrq != null and item.sjksrq != ''">
                    to_date(#{item.sjksrq}, 'yyyy/mm/dd'),
                </if>
                <if test="item.sjwcrq != null and item.sjwcrq != ''">
