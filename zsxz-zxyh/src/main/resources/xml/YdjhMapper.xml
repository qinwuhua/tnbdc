                </if>
                <if test="item.jhksrq != null and item.jhksrq != ''">
                    jhksrq,
                </if>
                <if test="item.jhwcrq != null and item.jhwcrq != ''">
                    jhwcrq,
                </if>
                <if test="item.sjgcl != null and item.sjgcl != ''">
                    sjgcl,
