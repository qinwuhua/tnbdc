        <result column="jhksrq" property="jhksrq"/>
        <result column="jhwcrq" property="jhwcrq"/>
    </resultMap>

    <select id="selectMx" resultMap="ydjhmxBean">
        select * from ZXYH_YDJH_MX where ydid in #{id}
    </select>

    <select id="getYdjh" resultMap="ydjhBean">
        select * from ZXYH_YDJH yd left join DJSJ_HTXX_HTGJXX ht on yd.djbh=ht.htgjxx_htbh
    </select>

    <insert id="addYdjh" parameterType="com.hdsx.zxyh.entity.Ydjh">
        insert into ZXYH_YDJH
        <trim prefix="(" suffix=")" suffixOverrides=",">
            <if test="id !=null and id !=''">
                id,
            </if>
            <if test="djbh !=null and djbh !=''">
                djbh,
            </if>
            <if test="yd != null and yd !=''">
                yd,
            </if>
            <if test="byjhwcqk !=null and byjhwcqk !=''">
                byjhwcqk,
            </if>
            <if test="xyjhwcqk != null and xyjhwcqk !=''">
                xyjhwcqk,
            </if>
            <if test="bz !=null and bz !=''">
                bz,
            </if>
            <if test="bzr !=null and bzr !=''">
                bzr,
            </if>
            <if test="bzrq !=null and bzrq !=''">
                bzrq,
            </if>
            <if test="bzdw !=null and bzdw !=''">
                bzdw,
            </if>
            <if test="bzbm !=null and bzbm !=''">
                bzbm,
            </if>
            <if test="sbzt != null and sbzt != ''">
                sbzt,
            </if>
            <if test="pfzt != null and pfzt != ''">
                pfzt,
            </if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="id !=null and id !=''">
                #{id},
            </if>
            <if test="djbh !=null and djbh !=''">
                #{djbh},
            </if>
            <if test="yd != null and yd !=''">
                #{yd},
            </if>
            <if test="byjhwcqk !=null and byjhwcqk !=''">
                #{byjhwcqk},
            </if>
            <if test="xyjhwcqk != null and xyjhwcqk !=''">
                #{xyjhwcqk},
            </if>
            <if test="bz !=null and bz !=''">
                #{bz},
            </if>
            <if test="bzr !=null and bzr !=''">
                #{bzr},
            </if>
            <if test="bzrq !=null and bzrq !=''">
                to_date(#{bzrq}, 'yyyy/mm/dd'),
            </if>
            <if test="bzdw !=null and bzdw !=''">
                #{bzdw},
            </if>
            <if test="bzbm !=null and bzbm !=''">
                #{bzbm},
            </if>
            <if test="sbzt != null and sbzt != ''">
                #{sbzt},
            </if>
            <if test="pfzt != null and pfzt != ''">
                #{pfzt},
            </if>
        </trim>
    </insert>

    <insert id="addYdjhmx" parameterType="list">
        BEGIN
        <foreach collection ="list" item="item" index= "index" separator =" ">
            insert into ZXYH_YDJH_MX
            <trim prefix="(" suffix=")" suffixOverrides=",">
                <if test="item.mxid !=null and item.mxid !=''">
                    mxid,
                </if>
                <if test="item.ydid !=null and item.ydid !=''">
                    ydid,
                </if>
                <if test="item.qdzh !=null and item.qdzh !=''">
                    qdzh,
                </if>
                <if test="item.zdzh != null and item.zdzh !=''">
                    zdzh,
                </if>
                <if test="item.jggc !=null and item.jggc !=''">
                    jggc,
                </if>
                <if test="item.fx !=null and item.fx !=''">
                    fx,
                </if>
                <if test="item.czfs !=null and item.czfs !=''">
                    czfs,
                </if>
                <if test="item.czcd !=null and item.czcd !=''">
                    czcd,
                </if>
                <if test="item.jhgcl !=null and item.jhgcl !=''">
