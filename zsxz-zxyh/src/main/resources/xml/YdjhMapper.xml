            <result column="htgjxx_jbbm" property="jbbm"/>
        </association>

        <!-- 一对多的关系 -->
        <!-- property: 指的是集合属性的值, ofType：指的是集合中元素的类型 -->
        <collection property="ydjhmxList" ofType="com.hdsx.zxyh.entity.Ydjhmx" select="selectMx" column="id"></collection>
    </resultMap>

    <resultMap id="ydjhmxBean" type="com.hdsx.zxyh.entity.Ydjhmx">
        <result column="mxid" property="mxid"/>
        <result column="ydid" property="ydid"/>
        <result column="qdzh" property="qdzh"/>
        <result column="zdzh" property="zdzh"/>
        <result column="jggc" property="jggc"/>
        <result column="fx" property="fx"/>
