        <!-- 一对一的关系 -->
        <!-- property: 指的是属性的值, javaType：指的是元素的类型 -->
        <association property="htgjxx" javaType="com.hdsx.zxyh.entity.Htgjxx">
            <id column="htgjxx_htbh" property="htbh"/>
            <result column="htgjxx_htmc" property="htmc"/>
